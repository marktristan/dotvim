execute pathogen#infect()

colorscheme jellybeans

macmenu &File.New\ Tab key=<nop>
map <D-t> <Plug>PeepOpen

set nocp
set bs=indent,eol,start
set gcr=n:blinkon0
set hls is ic scs
filetype plugin indent on
set sw=2 sts=2 et
set ruler
set guioptions=
set guifont=inconsolata:h12
set number
set showtabline=0
